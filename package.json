{
  "name": "vue-data-tabulator",
  "author": "Matthew April",
  "description": "Light weight data tables component for Vue 2",
  "version": "0.2.0",
  "homepage": "https://github.com/MattApril/vue-data-tabulator#readme",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattApril/vue-data-tabulator"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-data-tabulator ./src/main.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.5.17"
  },
  "main": "./dist/vue-data-tabulator.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "keywords": [
    "vue",
    "vue2",
    "data",
    "tables",
    "tabulator",
    "sortable",
    "sort"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/MattApril/vue-data-tabulator/issues"
  }
}
